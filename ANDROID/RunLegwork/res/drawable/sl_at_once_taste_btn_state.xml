<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_focused="true"><shape>
            <solid android:color="@color/at_once_taset_pressd" />

            <stroke android:width="1px" android:color="@color/at_once_taset_pressd" />
        </shape></item>
    <item android:state_pressed="true"><shape>
            <solid android:color="@color/at_once_taset_pressd" />

            <stroke android:width="1px" android:color="@color/at_once_taset_pressd" />
        </shape></item>
    <item><shape>
            <solid android:color="@color/at_once_taste_unpressd" />

            <stroke android:width="1px" android:color="@color/at_once_taste_unpressd" />
        </shape></item>

</selector>